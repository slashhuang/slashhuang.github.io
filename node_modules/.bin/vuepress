#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.13.5_jiti@2.4.2_lightn_zcjeavzfk7hzxb2fnungf2hkwu/node_modules/vuepress/bin/node_modules:/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.13.5_jiti@2.4.2_lightn_zcjeavzfk7hzxb2fnungf2hkwu/node_modules/vuepress/node_modules:/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.13.5_jiti@2.4.2_lightn_zcjeavzfk7hzxb2fnungf2hkwu/node_modules:/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.13.5_jiti@2.4.2_lightn_zcjeavzfk7hzxb2fnungf2hkwu/node_modules/vuepress/bin/node_modules:/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.13.5_jiti@2.4.2_lightn_zcjeavzfk7hzxb2fnungf2hkwu/node_modules/vuepress/node_modules:/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepress+bundler-vite@2.0.0-rc.20_@types+node@22.13.5_jiti@2.4.2_lightn_zcjeavzfk7hzxb2fnungf2hkwu/node_modules:/Users/huangxiaogang/work/slashhuang.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress.js" "$@"
fi
